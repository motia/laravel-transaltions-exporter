{
    "name": "motia/laravel-translation-exporter",
    "description": "Utility package to use your translations in your laravel backend and client app at the same time.",
    "keywords": ["translations", "i18n", "spa"],
    "license": "MIT",
    "require": {
        "vsch/laravel-translation-manager": "^2.6"
    },
    "require-dev": {
        "symfony/yaml": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Motia\\TransExport\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Motia\\TransExport\\TransExportProvider"
            ]
        }
    }
}
